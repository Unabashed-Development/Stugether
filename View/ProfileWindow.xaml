<Window x:Class="View.ProfileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewmodels="clr-namespace:ViewModel;assembly=ViewModel" xmlns:view="clr-namespace:View" d:DataContext="{d:DesignInstance Type=viewmodels:MainViewModel}"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Profiel van je match" Height="600" Width="800">
    <ScrollViewer>
        <Frame Content="{Binding CurrentViewModel}" NavigationUIVisibility="Hidden" x:Name="ProfileWindowFrame" x:FieldModifier="public"/>
    </ScrollViewer>
</Window>
